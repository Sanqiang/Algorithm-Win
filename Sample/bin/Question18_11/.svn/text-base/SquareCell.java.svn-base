package Question20_11;

public class SquareCell {
	public int zeros_right = 0;
	public int zeros_below = 0;
	public SquareCell(int right, int below) {
		zeros_right = right;
		zeros_below = below;
	}
	
	public void setZerosRight(int right) {
		zeros_right = right;
	}
	
	public void setZerosBelow(int below) {
		zeros_below = below;
	}
}
